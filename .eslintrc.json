{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "camelcase": "off",
    "no-param-reassign": "warn",
    "arrow-parens": ["error", "as-needed"]
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ]
}
