<!DOCTYPE html>
<html>
  <head>
	<!-- Page Title -->
    <title>Users</title>
		<% include ./partials/head %>
	</head>
  <body>
		<header>
			<% include ./partials/header %>
		</header>
		<div class="container">
    <h1>Users</h1>
	<table class="table">
		<thead>
			<tr>
				<th scope="col">User ID</th>
				<th scope="col">Name</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0; i<data.length; i++) { %>
			<tr>
				<td><%= data[i].id%></td>
				<td><%= data[i].name%></td>
			</tr>
			<% } %>
		</tbody>
	</table>
	</div>
	<footer>
		<% include ./partials/footer %>
	</footer>
	</body>
</html>
